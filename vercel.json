{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "FEISHU_APP_ID": "cli_a906a5b58876dbc7",
    "FEISHU_APP_SECRET": "SECRET_IN_ENV_VAR"
  }
}